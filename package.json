{
  "name": "@njfiorello/weather-gauges",
  "private": true,
  "version": "0.4.1",
  "license": "MIT",
  "description": "Some svg gauges relating to weather.",
  "keywords": [
    "weather",
    "angular",
    "svg",
    "gauges"
  ],
  "homepage": "https://melgish.github.io/weather-gauges/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib && npm run build:docs",
    "test": "jest -c projects/weather-gauges/jest.config.js",
    "lint": "ng lint",
    "version": "sync-json -v --property version --source package.json projects/weather-gauges/package.json",
    "build:lib": "ng build -c production weather-gauges",
    "build:docs": "ng build -c production weather-gauges-docs && cp docs/index.html docs/404.html",
    "ci:build": "npm run build",
    "ci:test": "jest --ci --coverage",
    "jest": "jest"
  },
  "dependencies": {
    "@angular/animations": "^12.2.10",
    "@angular/common": "^12.2.10",
    "@angular/compiler": "^12.2.10",
    "@angular/core": "^12.2.10",
    "@angular/forms": "^12.2.10",
    "@angular/localize": "^12.2.10",
    "@angular/platform-browser": "^12.2.10",
    "@angular/platform-browser-dynamic": "^12.2.10",
    "@angular/router": "^12.2.10",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "bootstrap": "^4.6.0",
    "prismjs": "^1.27.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.10",
    "@angular-eslint/builder": "12.7.0",
    "@angular-eslint/eslint-plugin": "12.7.0",
    "@angular-eslint/eslint-plugin-template": "12.7.0",
    "@angular-eslint/schematics": "12.7.0",
    "@angular-eslint/template-parser": "12.7.0",
    "@angular/cli": "^12.2.10",
    "@angular/compiler-cli": "^12.2.10",
    "@angular/language-service": "^12.2.10",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.20.33",
    "@types/prismjs": "^1.16.6",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "^7.26.0",
    "jest": "^28.1.3",
    "jest-junit": "^13.0.0",
    "jest-preset-angular": "^12.2.2",
    "ng-packagr": "^12.2.3",
    "prismjs-loader": "0.0.4",
    "sync-json": "^1.0.2",
    "ts-node": "^10.9.1",
    "typescript": "~4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/melgish/weather-gauges.git"
  }
}
