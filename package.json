    "protractor": "~5.1.2",
    "rxjs": "^5.5.8",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "zone.js": "^0.8.19"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/melgish/weather-gauges.git"
